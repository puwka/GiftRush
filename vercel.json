{
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/css/(.*)",
        "headers": { "Cache-Control": "public, max-age=31536000" }
      },
      {
        "src": "/js/(.*)",
        "headers": { "Cache-Control": "public, max-age=31536000" }
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ]
  }